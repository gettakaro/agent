{
  "name": "takaro-agent",
  "version": "0.1.0",
  "description": "AI agents for Takaro module development",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "tsc && cp -r src/views dist/ && cp -r src/public dist/",
    "start": "node dist/main.js",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:rollback": "tsx src/db/rollback.ts"
  },
  "dependencies": {
    "@takaro/apiclient": "^0.4.10",
    "axios": "^1.13.2",
    "bullmq": "^5.66.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "ioredis": "^5.8.2",
    "knex": "^3.1.0",
    "openai": "^4.77.0",
    "pg": "^8.13.1",
    "redis": "^5.10.0",
    "uuid": "^11.0.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.9",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2"
  }
}
